{
  "name": "portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4300 -o",
    "build": "ng build --base-href",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "compodoc": "npx compodoc -p tsconfig.json -s -w --theme readthedocs --hideGenerator --customFavicon src/favicon.ico --customLogo src/assets/images/logo/icon_w.png",
